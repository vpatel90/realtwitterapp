

<div class="thought-card col s12 m12 l12">
  <div class="row thought-header">
    <div class="col s6 m4 l4">
      <p> $<%= thought.user.handle %> </p>
    </div>

    <div class="right">
      <p> <small><%= thought.time %> </small></p>
    </div>

  </div>
  <div class="row">
    <div class="col s12 m12 l12">
      <p> <%= thought.body %> </p>
    </div>
  </div>
</div>
